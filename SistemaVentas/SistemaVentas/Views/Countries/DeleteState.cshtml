@model SistemaVentas.Data.Entities.State

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Esta seguro de querer borrar?</h3>
<div>
    
    <partial name="_StateDetails"/>
    
    <form asp-action="DeleteState">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" /> |
        <a asp-action="Details" asp-route-id="@Model?.Country.ID" class="btn btn-outline-success">Regresar</a>
    </form>
</div>

